<package>
  <description brief="rosfawkes">
     This package integrates Fawkes into ROS.
  </description>
  <author>Tim Niemueller</author>
  <license>GPLv2+</license>
  <review status="unreviewed" notes=""/>
  <url>http://www.fawkesrobotics.org/documentation/rosfawkes</url>
  <depend package="roscpp"/>
  <export>
    <cpp cflags="-I${prefix}/fawkes/src -I${prefix}/src -I${prefix}/fawkes/src/libs -I${prefix}/src/libs"
         lflags="-Wl,-rpath,${prefix}/lib -L${prefix}/lib" />
  </export>
</package>


